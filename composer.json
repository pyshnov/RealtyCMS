{
    "name": "pyshnov/realty",
    "description": "CMS для агенства недвижимости",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "^1.0",
        "wikimedia/composer-merge-plugin": "^1.0"
    },
    "replace": {
        "pyshnov/core": "~0.0.18"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        }
    },
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Pyshnov"
    }
}
